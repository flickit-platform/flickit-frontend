        "@mui/lab": "^7.0.0-alpha.0",
        "@mui/material": "^7.2.0",
        "@novu/notification-center": "^2.0.0-canary.0",
        "@sentry/react": "^7.99.0",
        "@sentry/vite-plugin": "^2.13.0",
        "@tiptap/extension-link": "2.0.0-beta.217",
        "@tiptap/extension-placeholder": "^2.11.2",
        "@tiptap/extension-table": "^2.11.2",
        "@tiptap/extension-table-cell": "^2.11.2",
        "@tiptap/extension-table-header": "^2.11.2",
        "@tiptap/extension-table-row": "^2.11.2",
        "@tiptap/extension-text-align": "2.0.0-beta.217",
        "@tiptap/react": "2.0.0-beta.217",
        "@tiptap/starter-kit": "2.0.0-beta.217",
        "axios": "1.3.2",
        "flagsmith": "^9.2.0",
        "i18next": "22.4.9",
        "keycloak-js": "^23.0.3",
        "nanoid": "4.0.1",
        "react": "18.2.0",
        "react-beautiful-dnd": "^13.1.1",
        "react-dom": "18.2.0",
        "react-dropzone": "^14.2.3",
        "react-hook-form": "7.43.1",
        "react-i18next": "12.1.5",
        "react-router-dom": "6.8.1",
        "react-toastify": "9.1.1",
        "react-transition-group": "4.4.5",
        "recharts": "2.4.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.10.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "13.4.0",
        "@testing-library/user-event": "14.4.3",
        "@types/lodash": "4.14.191",
        "@types/node": "18.13.0",
        "@types/react": "18.0.28",
        "@types/react-beautiful-dnd": "^13.1.8",
        "@types/react-dom": "18.0.10",
        "@vitejs/plugin-react-swc": "3.1.0",
        "@vitest/coverage-c8": "0.28.4",
        "cypress": "12.5.1",
        "eslint": "^9.10.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.35.2",
        "globals": "^15.9.0",
        "husky": "^9.1.5",
        "jsdom": "21.1.0",
        "lint-staged": "^15.2.10",
        "prettier": "^3.3.3",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-replace": "^2.2.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-visualizer": "^5.9.0",
