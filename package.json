        "@mui/lab": "^7.0.1-beta.18",
        "@mui/material": "^7.3.4",
        "@mui/x-tree-view": "^8.14.1",
        "@novu/notification-center": "^2.0.0",
        "@sentry/react": "^10.21.0",
        "@sentry/vite-plugin": "^4.5.0",
        "@tiptap/extension-link": "3.7.2",
        "@tiptap/extension-placeholder": "^3.7.2",
        "@tiptap/extension-table": "^3.7.2",
        "@tiptap/extension-table-cell": "^3.7.2",
        "@tiptap/extension-table-header": "^3.7.2",
        "@tiptap/extension-table-row": "^3.7.2",
        "@tiptap/extension-text-align": "3.7.2",
        "@tiptap/react": "3.7.2",
        "@tiptap/starter-kit": "3.7.2",
        "axios": "1.12.2",
        "flagsmith": "^9.3.2",
        "i18next": "25.6.0",
        "keycloak-js": "^26.2.1",
        "nanoid": "5.1.6",
        "react": "19.2.0",
        "react-beautiful-dnd": "^13.1.1",
        "react-dom": "19.2.0",
        "react-dropzone": "^14.3.8",
        "react-hook-form": "7.65.0",
        "react-i18next": "16.1.5",
        "react-router-dom": "7.9.4",
        "react-toastify": "11.0.5",
        "react-transition-group": "4.4.5",
        "recharts": "3.3.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.38.0",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "16.3.0",
        "@testing-library/user-event": "14.6.1",
