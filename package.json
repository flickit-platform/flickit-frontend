    "@tiptap/starter-kit": "3.8.0",
    "axios": "1.12.2",
    "flagsmith": "^9.3.2",
    "i18next": "25.6.0",
    "keycloak-js": "^23.0.3",
    "material-ui-popup-state": "^5.3.6",
    "nanoid": "5.1.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "7.65.0",
    "react-i18next": "16.2.0",
    "react-router-dom": "7.9.4",
    "react-toastify": "11.0.5",
    "react-transition-group": "4.4.5",
    "recharts": "3.3.0"
